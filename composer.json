{
	"name" : "nvbooster/mongator-bundle",
	"description" : "Bundle to use Mongator with Symfony2",
	"keywords" : [
		"mongodb",
		"mongator"
	],
	"type" : "symfony-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Pablo DÃ­ez",
			"email" : "pablodip@gmail.com"
		}, {
			"name" : "Dario Mancuso",
			"email" : "dario@ewide.eu"
		}, {
			"name" : "Vyacheslav Nechipurenko",
			"email" : "nvb@aproxima.ru"
		}
	],
	"require" : {
		"php" : ">=5.6.31",
		"symfony/framework-bundle" : "2.*",
		"symfony/console" : "2.*",
		"symfony/validator" : "2.*",
		"mongator/mongator" : "^2.0",
		"twig/twig" : "~1.2||~2.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.1.*",
		"mockery/mockery" : "0.9.*",
		"symfony/form" : "2.*",
		"symfony/phpunit-bridge" : "~2.8"
	},
	"conflict" : {
		"mongator/mongator-bundle" : "*"
	},
	"replace" : {
		"mongator/mongator-bundle" : "self.version"
	},
	"autoload" : {
		"psr-4" : {
			"Mongator\\MongatorBundle\\" : ""
		}
	},
	"minimum-stability" : "dev",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.x-dev"
		}
	}
}